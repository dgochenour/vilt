<?xml version="1.0"?>
<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:noNamespaceSchemaLocation="http://community.rti.com/schema/6.0.1/rti_dds_qos_profiles.xsd">

    <qos_library name="ProximityDatatype_Library">

        <qos_profile name="ProximityDatatype_Profile" base_name="BuiltinQosLib::Generic.BestEffort">
                
            <datawriter_qos>
                <publication_name>
                    <name>OurProximityDatatypeDataWriter</name>
                </publication_name>
            </datawriter_qos>
             
            <datareader_qos>
                <subscription_name>
                    <name>OurProximityDatatypeDataReader</name>
                </subscription_name>
            </datareader_qos>

            <participant_qos>
                <!--
                The participant name, if it is set, will be displayed in the
                RTI tools, making it easier for you to tell one
                application from another when you're debugging.
                -->
                <participant_name>
                    <name>ProximityDatatypeParticipant</name>
                    <role_name>ProximityDatatypeParticipantRole</role_name>
                </participant_name>

            </participant_qos>

        </qos_profile>

    </qos_library>

</dds>
